#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,ok,i,d,a;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        cin>>a;
        ok=1;
        for(d=2;d*d<=a;d++)
        {
            if(a%d==0)
            {
                ok=0;
            }
        }
        if(ok==0)
        {
            cout<<1;
        }
        else
        {
            cout<<0;
        }
    }
    return 0;

}
